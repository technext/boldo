mixin Numbers
  -
    const numbers = [
      {
        number: "120M",
        title:'Cool feature title'
      },
      {
        number: "10.000",
        title:'Cool feature title'
      },
      {
        number: "240",
        title:'Cool feature title'
      },
    ]
  .bg-dark
    +Section(class="minus-margin")
      p.text-center.text-light Our numbers
      h1.text-center.text-white.w-lg-75.w-100.mx-auto.fs-xl-6.fs-lg-4.fs-3.lh-base Handshake infographic mass market crowdfunding iteration.
      .row.mt-5
        each number in numbers
          .col-xl-4.col-md-6.font-extra-learge
            h1.text-success= number.number
            p.text-light.fs-2= number.title