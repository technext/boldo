mixin Navbar(navbarDark, pageTitle)
    -
        var navLinks = [
            {
                href:"index.html",
                title:"Home"
            },
            {
                href:"about.html",
                title:"About"
            },
            {
                href:"blogs.html",
                title:"Blogs"
            }
        ]
    if navbarDark
        nav.navbar.navbar-expand-lg.fixed-top.w-100.py-4.navbar-dark(data-navbar-on-scroll)
            .container.px-lg-9.px-md-5
                a.navbar-brand(href="index.html")
                    img(src="assets/img/Logo.png", alt="logo")
                button.navbar-toggler(type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation")
                    i(class="fa-solid fa-bars").text-white.fs-3
                .collapse.navbar-collapse#navbarSupportedContent
                    ul.navbar-nav.ms-auto.mb-2.mb-lg-0
                        each link in navLinks 
                            li.nav-item
                                a.nav-link.mx-2.text-light(aria-current="page" href=link.href class= pageTitle === link.title ? "active" : "")= link.title
                        li.nav-item
                            a.nav-link.btn.bg-white.px-4.text-black.ms-2.w-md-25.w-50.w-lg-100(aria-current="page" href="#") Log In
    else 
        nav.navbar.navbar-expand-lg.fixed-top.w-100.py-4.navbar-light.bg-primary(data-navbar-on-scroll)
            .container.px-lg-9.px-md-5
                a.navbar-brand(href="#")
                    img(src="assets/img/Logo.png", alt="logo")
                button.navbar-toggler(type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation")
                    i(class="fa-solid fa-bars").text-white.fs-3
                .collapse.navbar-collapse#navbarSupportedContent
                    ul.navbar-nav.ms-auto.mb-2.mb-lg-0
                        each link in navLinks 
                            li.nav-item
                                a.nav-link.mx-2.text-light(aria-current="page" href=link.href class= pageTitle === link.title ? "active" : "")= link.title
                        li.nav-item
                            a.nav-link.btn.bg-white.px-4.text-black.ms-2.w-md-25.w-50.w-lg-100(aria-current="page" href="#") Log In